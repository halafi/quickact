{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run webpack:dev",
    "build": "npm run clean && npm run webpack:prod && npm run copy:assets",
    "clean": "rm -rf dist",
    "copy:assets": "cp src/client/index.html ./dist && cp -R public/* ./dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "webpack:dev": "webpack-dev-server --mode development --hot --inline --config config/webpack.dev.js --open --env.NODE_ENV=development",
    "webpack:prod": "webpack --mode production --config config/webpack.prod.js --env.NODE_ENV=production",
    "eslint": "eslint --ext .ts --ext .tsx .",
    "eslint:fix": "eslint --fix --ext .ts --ext .tsx .",
    "types": "tsc --noEmit",
    "types:watch": "tsc --noEmit --watch",
    "lint": "npm run eslint && npm run types"
  },
  "author": "Filip Halas",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.6.4",
    "date-fns": "^2.11.1",
    "jest": "^25.2.4",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "rebass": "^4.0.7",
    "regenerator-runtime": "^0.13.5",
    "styled-components": "^5.0.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/ramda": "^0.27.0",
    "@types/react": "^16.9.29",
    "@types/react-dom": "^16.9.5",
    "@types/rebass": "^4.0.4",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "assets-webpack-plugin": "^3.9.12",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-ramda": "^2.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "compression-webpack-plugin": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest-styled-components": "^7.0.0",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
